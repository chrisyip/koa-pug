{
  "name": "koa-pug",
  "description": "A Pug middleware for Koa",
  "version": "5.1.0",
  "license": "MIT",
  "main": "dist/index",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepublishOnly": "npm run lint && npm run build",
    "test": "ava",
    "lint": "tslint --format stylish --project .",
    "build": "npm run compile",
    "compile": "npm run clean && tsc",
    "clean": "del dist",
    "docs": "typedoc source"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@sindresorhus/is": "^0.15.0",
    "@sindresorhus/tsconfig": "^0.4.0",
    "@types/cheerio": "^0.22.12",
    "@types/extend": "^3.0.4",
    "@types/fs-extra": "^8.0.0",
    "@types/got": "^9.6.5",
    "@types/koa": "^2.13.5",
    "@types/koa-router": "^7.0.42",
    "@types/node": "^10.12.21",
    "@types/pug": "^2.0.4",
    "ava": "^3.8.2",
    "cheerio": "^1.0.0-rc.3",
    "del-cli": "^1.1.0",
    "got": "^9.6.0",
    "jstransformer-markdown-it": "^2.1.0",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.12.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.17.7",
    "typescript": "^3.8.3"
  },
  "types": "dist",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "dependencies": {
    "camelcase": "^6.3.0",
    "extend": "^3.0.2",
    "is-plain-obj": "^3.0.0",
    "pug": "^3.0.2"
  }
}
